{
    "name": "producttrap/driver-target-australia",
    "type": "library",
    "description": "A Target Australia driver for ProductTrap",
    "license": "MIT",
    "keywords": [
        "producttrap-driver",
        "target",
        "australia"
    ],
    "require": {
        "php": "^8.1",
        "graham-campbell/guzzle-factory": "^5.0",
        "producttrap/producttrap": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "orchestra/testbench": "^7.9",
        "pestphp/pest": "^1.21.1",
        "phpstan/phpstan": "^1.4.5",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ProductTrap\\TargetAustralia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProductTrap\\TargetAustralia\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test:types": "phpstan analyse --ansi --memory-limit=-1",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:types",
            "@test:unit"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProductTrap\\TargetAustralia\\TargetAustraliaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
